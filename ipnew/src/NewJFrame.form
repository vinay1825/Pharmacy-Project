    private void jButton5ActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jButton5ActionPerformed
    String a=jTextField8.getText();
    String b=new String(jPasswordField1.getPassword());
    try{
            Class.forName("java.sql.Driver");
            Connection con=DriverManager.getConnection("jdbc:mysql://localhost:3306/ipnew","root","student");
            Statement st=con.createStatement();
            String query="select * from users where username='"+a+"';";
            ResultSet rs=st.executeQuery(query);
            if(rs.next()){
                if(b.equals(rs.getString("password"))){
                    jFrame1.setVisible(true);
                    this.dispose();
                }
                else{
                       JOptionPane.showMessageDialog(null,"Wrong Password");
                       jTextField8.setText("");
                       jPasswordField1.setText("");
                       jTextField8.requestFocus();
            }
        }
        else{
                       JOptionPane.showMessageDialog(null,"Wrong username");
                       jTextField8.setText("");
                       jPasswordField1.setText("");
                       jTextField8.requestFocus();
        }
    }
    catch(Exception ex){
                JOptionPane.showMessageDialog(null,ex.getMessage());
    }

    }//GEN-LAST:event_jButton5ActionPerformed

