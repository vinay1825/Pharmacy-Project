    String a=jTextField9.getText();
    String b=new String(jPasswordField2.getPassword());
    String c=new String(jPasswordField3.getPassword());
    String d=new String(jPasswordField4.getPassword());
    if(c.equals(d))
    {
    try{
            Class.forName("java.sql.Driver");
            Connection con=DriverManager.getConnection("jdbc:mysql://localhost:3306/ipnew","root","student");
            Statement st=con.createStatement();
            String query="select * from users where username='"+a+"' and password='"+b+"';";
            ResultSet rs=st.executeQuery(query);
            if(rs.next()){
                    st.executeUpdate("update users set password='"+c+"' where username='"+a+"';");
                    JOptionPane.showMessageDialog(null,"Password has been changed.");
                }
                else{
                    JOptionPane.showMessageDialog(null,"Username or old password is not correct.");
                }
        }
    catch(Exception ex){
                JOptionPane.showMessageDialog(null,ex.getMessage());
    }
    }
    else
    {
        JOptionPane.showMessageDialog(null,"New Password does not match with Re-entered password");
    }
    }//GEN-LAST:event_jButton22ActionPerformed

